// schema.prisma

datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model FacebookEvent {
  id          String   @id @default(uuid())
  eventId     String   @unique
  timestamp   DateTime @db.Timestamp()
  funnelStage String
  eventType   String
  data        Json

  @@map("facebookevent")
}